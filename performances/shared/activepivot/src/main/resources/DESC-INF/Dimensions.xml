<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<dimensions xmlns="http://www.quartetfs.com"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.quartetfs.com ../activepivot-types.xsd">
	
	<!--Your dimensions here-->

	<dimension name="Sectors">
		<hierarchy name="SectorHierarchy">
			<level name="Sector" />
			<level name="Industry" />
			<level name="CompanyName" />
		</hierarchy>
	</dimension>

	<dimension name="Time" type="TIME">
		<hierarchy name="TimeHierarchy" allMember="false">
			<level name="Date" property="Date" type="TIME" formatter="DATE[yyyy-MM-dd]">
				<comparator pluginKey="ReverseOrder" />
			</level>
		</hierarchy>
	</dimension>

	<dimension name="Forex">
		<hierarchy name="ForexHierarchy" allMember="false" factless="true">
			<properties>
				<entry key="storeName" value="Forex" />
			</properties>
			<level name="Currency">
				<properties>
					<entry key="fieldName" value="CurrencyPair" />
				</properties>
			</level>
		</hierarchy>
	</dimension>

	<dimension name="PositionTypes">
		<hierarchy name="PositionTypeHierarchy">
			<level name="PositionType"/>
		</hierarchy>
	</dimension>

	<dimension name="StockSymbols">
		<hierarchy name="StockSymbolHierarchy">
			<level name="StockSymbol" />
		</hierarchy>
	</dimension>

	<dimension name="PortfolioTypes">
		<hierarchy name="PortfolioTypeHierarchy">
			<level name="PortfolioType"/>
		</hierarchy>
	</dimension>

</dimensions>