<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<dimensions xmlns="http://www.quartetfs.com"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.quartetfs.com ../activepivot-types.xsd">
	
	<!--Your dimensions here-->
	<dimension name="CompaniesRegroupment">
		<hierarchy name="CompaniesRegroupment">
			<level name="Sector"/>
			<level name="Industry"/>
            <level name="Name"/>
		</hierarchy>
	</dimension>

    <dimension name="Time" type="TIME">
        <hierarchy name="HistoricalDates" allMember="false">

            <level name="Date" type="TIME" formatter="DATE[yyyy-MM-dd]">
                <!-- add comparator -->
                <!-- <comparator pluginKey="ReverseOrder" />-->
            </level>
        </hierarchy>
    </dimension>

    <dimension name="StockSymbol" />

    <dimension name="IndexName" />



    <dimension name="ForexDim">
        <!--TODO : allMember="false" => No sense to keep it !
            The Context Value guaranty that a currency will al-->
        <hierarchy name="ForexHier" allMember="true" factless="true">
            <properties>
                <entry key="storeName" value="Forex"/>
            </properties>
            <level name="Currency">
                <properties>
                    <entry key="fieldName" value="TargetCurrency"/>
                </properties>
            </level>
        </hierarchy>
    </dimension>

</dimensions>