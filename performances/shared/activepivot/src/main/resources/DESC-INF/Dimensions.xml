<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<dimensions xmlns="http://www.quartetfs.com"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.quartetfs.com ../activepivot-types.xsd">
	
	<!--Group of hierarchy for sector & industry-->
	<dimension name="SectorDimension">
		<hierarchy name="SectorHierarchy">
			<level name="Sector" /> <!-- should be the same value as in cube schema -->
			<level name="Industry" />
            <level name="CompanyName" />
            <level name="StockSymbol" />
		</hierarchy>
	</dimension>

    <dimension name="IndustryDimension">
        <hierarchy name="IndustryHierarchy">
            <level name="Industry" />
        </hierarchy>
    </dimension>

    <!-- group of hierarchy for time -->
    <dimension name="TimeDimension" type="TIME">
        <hierarchy name="TimeHierarchy" allMember="false">
            <level name="Date" property="Date" type="TIME" formatter="DATE[yyyy-MM-dd]">
                <comparator pluginKey="ReverseOrder" />
                
            </level>
        </hierarchy>
    </dimension>
    
    <dimension name="StockSymbol" />

    <dimension name="StockSymbolI" />
    
    <dimension name="PortfolioType" />

    <dimension name="IndexName" />

    <dimension name="ForexDimension">
        <hierarchy name="ForexHierarchy" allMember="false" factless="true">
            <properties>
                <entry key="storeName" value="Forex" />
            </properties>
            <level name="Currency">
                <properties>
                    <entry key="fieldName" value="Target currency" />
                </properties>
            </level>
        </hierarchy>
    </dimension>

</dimensions>