<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<pivotDescription xmlns="http://www.quartetfs.com"
                  xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://www.quartetfs.com activepivot-types.xsd">

    <!-- Selection of the aggregate provider -->
    <!--<aggregateProvider pluginKey="BITMAP"/>-->
    <!--<aggregateProvider pluginKey="JUST_IN_TIME" />-->

    <!--<aggregateProvider pluginKey="BITMAP">-->
        <!--<properties>-->
            <!--<entry key="rangeSharing" value="1_000_000" />-->
        <!--</properties>-->
    <!--</aggregateProvider>-->

    <aggregateProvider pluginKey="JUST_IN_TIME" >
        <properties>
            <entry key="rangeSharing" value="1_000_000" />
        </properties>

        <partialProvider pluginKey="BITMAP">
            <hierarchies>
                <hierarchy name="SectorHierarchy" dimension="SectorDimension" level="Sector"/>

            </hierarchies>
            <measures>
                <measure name="Close.sum" />
                <measure name="Close.Avg" />
                <measure name="HighValue.Sum" />
                <measure name="HighValue.Avr" />
                <measure name="LowValue.Sum" />
                <measure name="LowValue.Avg" />
                <measure name="Open.Sum" />
                <measure name="Open.Avr" />
                <measure name="Volume.Sum" />
                <measure name="AdjClose.Sum" />
                <measure name="AdjClose.Avg" />
                <measure name="NumberStocks" />
            </measures>
        </partialProvider>
    </aggregateProvider>

    <!-- true: Activation of factless hierarchies, across all dimensions -->
    <autoFactlessHierarchies>true</autoFactlessHierarchies>

    <!-- Definition of the dimensions -->
    <dimensions>
        <import resource="DESC-INF/Dimensions.xml"/>
    </dimensions>


    <!-- Definition of the measures -->
    <measures>
        <import resource="DESC-INF/Measures.xml"/>
    </measures>

    <aggregatesCacheDescription size="100"/>

    <sharedContexts>
        <!-- Query maximum execution time (before timeout cancellation) -->
        <queriesTimeLimit seconds="60"/>

        <!-- Contextual configuration of the MDX engine -->
        <mdxContext>
            <formatters>
                <!-- put formatter here (or in measures.xml) -->
                <formatter uniqueName="[Measures].[Close.Avg]" format="DOUBLE[#,###.0000;-#,###.0000]" />

            </formatters>
        </mdxContext>

        <ReferenceCurrency referenceCurrency="EUR"/>
    </sharedContexts>

</pivotDescription>
