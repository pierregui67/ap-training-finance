<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<pivotDescription xmlns="http://www.quartetfs.com"
                  xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://www.quartetfs.com activepivot-types.xsd">



    <!-- Selection of the aggregate provider -->
    <aggregateProvider pluginKey="JUST_IN_TIME">
        <properties>
            <entry key="rangeSharing" value="true" />
        </properties>

        <partialProvider pluginKey="BITMAP">
            <hierarchies>
                <hierarchy name="IndexName"/>
                <hierarchy name="HistoricalDates" />
            </hierarchies>
            <measures>
                <measure name="Close.SUM"/>
                <measure name="Volume.SUM" />
                <measure name="NumberStocks.SUM" />
                <measure name="PortfolioValue" />
                <measure name="CumulativeReturn" />
                <measure name="CumulativeReturnPercent" />
                <measure name="PeriodReturn" />
                <measure name="PeriodReturnPercent" />
                <measure name="CompareExample2Portfolio" />
                <measure name="CompareExample2PortfolioPercent" />
            </measures>
        </partialProvider>

    </aggregateProvider>

    <!--Sharing the computed data and using the cache-->
    <!--<aggregatesCacheDescription size="0" include="true" selectedMeasures="CompareExample2Portfolio, PeriodReturn, CumulativeReturn, PortfolioValue"/>
-->
    <!-- true: Activation of factless hierarchies, across all dimensions -->
    <autoFactlessHierarchies>true</autoFactlessHierarchies>

    <!-- Definition of the dimensions -->
    <dimensions>
        <import resource="DESC-INF/Dimensions.xml"/>
    </dimensions>


    <!-- Definition of the measures -->
    <measures>
        <import resource="DESC-INF/Measures.xml"/>
    </measures>

    <!-- Selection of the continuous query engine -->
    <aggregatesContinuousQueryEngine pluginKey="CONFLATE" />

    <sharedContexts>
        <!-- Query maximum execution time (before timeout cancellation) -->
        <queriesTimeLimit seconds="60"/>

        <!-- Contextual configuration of the MDX engine -->
        <mdxContext>
            <formatters>
                <formatter uniqueName="Volume.SUM" format="DOUBLE[#,###.00;-#,###.00]" />
                <formatter uniqueName="Forex.AVG" formatter="DOUBLE[#,###.00;-#,###.00]" />
            </formatters>
        </mdxContext>

        <referenceCurrency currency="EUR"/>


    </sharedContexts>

</pivotDescription>
