<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contexts xmlns="http://www.quartetfs.com"
          xmlns:xs="http://www.w3.org/2001/XMLSchema"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://www.quartetfs.com ../../activepivot-types.xsd">

    <subCubeProperties isAccessGranted="true">
        <grantedMeasures>
            <measure>Close.SUM</measure>
            <measure>PeriodReturn</measure>
            <measure>CumulativeReturn</measure>
            <measure>PortfolioValue</measure>
            <measure>Volume.SUM</measure>
            <measure>NumberStocks</measure>
        </grantedMeasures>
        <hierarchyRestrictions>
            <hierarchy  dimension="IndexName" name="IndexName">
                <allowedMember>
                    <memberPathPart xsi:type="xs:string" >AllMember</memberPathPart>
                    <memberPathPart xsi:type="xs:string" >benchmark</memberPathPart>
                </allowedMember>
            </hierarchy>
        </hierarchyRestrictions>
    </subCubeProperties>
</contexts>